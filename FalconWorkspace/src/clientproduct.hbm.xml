<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "dB.clientproduct" table = "ClientProduct">
      
      <meta attribute = "class-description">
         This class contains the employee detail. 
      </meta>
      
      <id name = "GroupNumber" type = "string" column = "GroupNumber">
         <generator class="native"/>
      </id>
      
      <property name = "SourceID" column = "SourceID" type = "string"/>
      <property name = "ClientID" column = "ClientID" type = "string"/>
      <property name = "ClientEffectiveDate" column = "CLientEffectiveDate" type = "string"/>
      <property name = "ProductType" column = "ProductType" type = "string"/>
      <property name = "BillingType" column = "BillingType" type = "string"/>
      <property name = "Percent" column = "Percent" type = "string"/>
      <property name = "ProdEffectiveDate" column = "ProdEffectiveDate" type = "string"/>
      <property name = "ProdTerminateDate" column = "ProdTerminateDate" type = "string"/>
     
      
   </class>
</hibernate-mapping>